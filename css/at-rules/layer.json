{
  "css": {
    "at-rules": {
      "layer": {
        "__compat": {
          "description": "<code>@layer</code>",
          "mdn_url": "https://developer.mozilla.org/docs/Web/CSS/@layer",
          "spec_url": "https://drafts.csswg.org/css-cascade-5/#layering",
          "support": {
            "chrome": {
              "version_added": "97",
              "flags": [
                {
                  "type": "preference",
                  "name": "enable-cascade-layers",
                  "value_to_set": "true"
                }
              ]
            },
            "chrome_android": {
              "version_added": false
            },
            "edge": {
              "version_added": false
            },
            "firefox": {
              "version_added": "94",
              "flags": [
                {
                  "type": "preference",
                  "name": "layout.css.cascade-layers.enabled",
                  "value_to_set": "true"
                }
              ]
            },
            "firefox_android": {
              "version_added": "94",
              "flags": [
                {
                  "type": "preference",
                  "name": "layout.css.cascade-layers.enabled",
                  "value_to_set": "true"
                }
              ]
            },
            "ie": {
              "version_added": false
            },
            "opera": {
              "version_added": false
            },
            "opera_android": {
              "version_added": false
            },
            "safari": {
              "version_added": false
            },
            "safari_ios": {
              "version_added": false
            },
            "samsunginternet_android": {
              "version_added": false
            },
            "webview_android": {
              "version_added": false
            }
          },
          "status": {
            "experimental": true,
            "standard_track": true,
            "deprecated": false
          }
        }
      }
    }
  }
}
