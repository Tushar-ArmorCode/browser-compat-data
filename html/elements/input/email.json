{
  "html": {
    "elements": {
      "input": {
        "input-email": {
          "__compat": {
            "mdn_url": "https://developer.mozilla.org/docs/Web/html/Element/input/email",
            "support": {
              "chrome": {
                "version_added": "5"
              },
              "chrome_android": {
                "version_added": null
              },
              "edge": {
                "version_added": null
              },
              "edge_mobile": {
                "version_added": null
              },
              "firefox": {
                "version_added": null,
                "notes": "This feature is not implemented yet. See <a href='https://bugzil.la/888320'>bug 888320</a> and <a href='https://wiki.mozilla.org/TPE_DOM/Date_time_input_types'>TPE DOM/Date time input types</a>."
              },
              "firefox_android": {
                "version_added": "4"
              },
              "ie": {
                "version_added": "10"
              },
              "opera": {
                "version_added": "11"
